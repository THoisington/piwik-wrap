{
    "name": "piwik-wrap",
    "version": "0.0.5",
    "description": "An easy to use wrapper for the piwik-client.",
    "main": "build/Piwik.js",
    "scripts": {
        "test": "node node_modules/.bin/mocha ./test/src --recursive --compilers js:babel/register",
        "watch-test": "node node_modules/.bin/mocha ./test/src --recursive --compilers js:babel/register --watch --growl",
        "build": "node node_modules/.bin/babel ./src --out-dir ./build",
        "postinstall": "node node_modules/.bin/babel ./src --out-dir ./build"
    },
    "keywords": [
        "piwik",
        "client",
        "browser",
        "tracking",
        "wrapper",
        "proxy"
    ],
    "license": "Unlicense",
    "author": "peerigon <developers@peerigon.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/peerigon/piwik-wrap.git"
    },
    "dependencies": {
        "babel": "^5.8.23"
    },
    "devDependencies": {
        "express": "^4.13.3",
        "jsdom": "^6.5.1",
        "mocha": "^2.3.3",
        "sinon": "^1.17.2",
        "unexpected": "^10.0.1"
    }
}
